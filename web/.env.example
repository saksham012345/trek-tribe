# Trek Tribe Web Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration - Update this with your deployed API URL
REACT_APP_API_URL=http://localhost:4000

# App Configuration
REACT_APP_APP_NAME=Trek Tribe
REACT_APP_VERSION=1.0.0

# Build Configuration for Production
GENERATE_SOURCEMAP=false
TSC_COMPILE_ON_ERROR=true
ESLINT_NO_DEV_ERRORS=true

# For Vercel Production Deployment:
# Set REACT_APP_API_URL to your deployed API URL in Vercel dashboard
# Example: REACT_APP_API_URL=https://your-api-domain.vercel.app
# Or if using Render: REACT_APP_API_URL=https://trek-tribe-api.onrender.com

# Socket.IO Configuration (if needed)
# REACT_APP_SOCKET_URL=https://trek-tribe-api.onrender.com
