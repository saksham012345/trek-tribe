{
  "version": 2,
  "builds": [
    {
      "src": "dist/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/serverless.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "functions": {
    "dist/serverless.js": {
      "maxDuration": 30
    }
  }
}
